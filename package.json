{
  "name": "parcel-starter",
  "version": "1.0.0",
  "description": "Static site starter with Parcle bundler.",
  "scripts": {
    "start": "rimraf ./dev && parcel serve ./src/*.html --dist-dir dev",
    "build": "rimraf ./dist && parcel build ./src/*.html --dist-dir dist --public-url ./"
  },
  "author": "Hasib",
  "license": "MIT",
  "browserslist": "last 4 version",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "autoprefixer": "^10.2.5",
    "parcel": "^2.0.0-beta.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^5.0.0"
  }
}
